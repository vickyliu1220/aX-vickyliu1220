print_df_info <- function(df) {
print("---------------")
print("Data frame info")
print(paste("   num. rows:     ", nrow(df)))
print(paste("   num. cols:     ", ncol(df)))
print("   column names:")
print(colnames(df))
print("   row names:")
print(head(rownames(df),4))
print("---------------")
print(str(df))
print("---------------")
print(sapply(df, class))
print("---------------")
}
